<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" >
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="Content-Type" content="application/xhtml+xml; charset=UTF-8" />
    <meta name="title" content="@ViewBag.MetaTitle" />
    <meta name="keywords" content="@ViewBag.MetaKeywords" />
    <meta name="author" content="@ViewBag.MetaAuthor" />
    <link rel="shortcut icon" href="@Url.Content("~/favicon.ico")" />
    <link href="@Url.Content("~/content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
</head>
<body>
<div id="container">
    <div id="header">
        <h1>
            <a title="@ViewBag.Setting.Title_set" href="@ViewBag.Setting.WebUrl_set"><img src="@Url.Content("~/content/images/logo.jpg")" alt="@ViewBag.Setting.Title_set" /></a></h1>
        @Html.Partial("_LogOnPartial")
        <div class="tel"><img src="@Url.Content("~/content/images/tel.gif")" alt="<=Tel>：400-887-4949" />400-887-4949</div>
    </div>
    <div class="menu">
        <ul>
            <li class="current"><a href="@Url.Content("~/")"><=Home></a></li>
            <li><a href="@Url.Content("~/")company.html"><=Company></a></li>
            <li><a href="@Url.Content("~/")news"><=News></a></li>
            <li><a href="@Url.Content("~/")job"><=Job></a></li>
            <li><a href="http://www.moresoft.cn"><=Forum></a></li>
            <li><a href="@Url.Content("~/")service.html"><=Service></a></li>
        </ul>
    </div>
    <div id="wrapper">
        @RenderBody()
    </div>
    <div id="footer">
        @if (ViewBag.Link!=null){
        <ul class="link">@foreach (var item in ViewBag.Link)
               { <li><a href="@Url.Content("~/")link.aspx?url=@item.Url_lnk" target="_blank">@item.Name_lnk</a></li>
        }</ul>}
        <div class="clear"></div>
        <p>
            <a href="@Url.Content("~/")product"><=Product></a> | 
            <a href="@Url.Content("~/")industry.html"><=Industry></a> | 
            <a href="@Url.Content("~/")service.html"><=Service1></a> | 
            <a href="@Url.Content("~/")partners.html"><=Partners></a> | 
            <a href="@Url.Content("~/")case.html"><=Case></a> | 
            <a href="@Url.Content("~/")news"><=News1></a> | 
            <a href="@Url.Content("~/")company.html"><=Company></a> | 
            <a href="@Url.Content("~/")concact.html"><=Concact></a> | 
            <a href="@Url.Content("~/")sitemap.html"><=SiteMap></a> | 
            <a href="@Url.Content("~/")download.html"><=Download></a>
        </p>
        <p><=CopyRight> <a href="http://www.miibeian.gov.cn/">@ViewBag.Setting.ICP_set</a></p>
        <p><=CompanyAddress></p>
    </div>
</div>
</body>
</html>
